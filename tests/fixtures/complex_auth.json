{
  "openapi": "3.0.3",
  "info": {
    "title": "Complex Auth API",
    "version": "2.0.0",
    "description": "API with multiple security schemes for testing auth extraction"
  },
  "servers": [
    {
      "url": "https://api.example.com",
      "description": "Main server"
    }
  ],
  "security": [
    {
      "apiKeyHeader": []
    }
  ],
  "paths": {
    "/public": {
      "get": {
        "operationId": "getPublicData",
        "summary": "Public endpoint",
        "tags": ["public"],
        "security": [],
        "responses": {
          "200": {
            "description": "Public data"
          }
        }
      }
    },
    "/basic-protected": {
      "get": {
        "operationId": "getBasicProtected",
        "summary": "Basic auth endpoint",
        "tags": ["protected"],
        "security": [
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Protected data"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/bearer-protected": {
      "get": {
        "operationId": "getBearerProtected",
        "summary": "Bearer auth endpoint",
        "tags": ["protected"],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Protected data"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/api-key-query": {
      "get": {
        "operationId": "getApiKeyQuery",
        "summary": "API key in query string",
        "tags": ["protected"],
        "security": [
          {
            "apiKeyQuery": []
          }
        ],
        "responses": {
          "200": {
            "description": "Protected data"
          }
        }
      }
    },
    "/oauth-protected": {
      "get": {
        "operationId": "getOAuthProtected",
        "summary": "OAuth2 protected endpoint",
        "tags": ["protected"],
        "security": [
          {
            "oauth2AuthCode": ["read", "write"]
          }
        ],
        "responses": {
          "200": {
            "description": "OAuth protected data"
          }
        }
      },
      "post": {
        "operationId": "createOAuthProtected",
        "summary": "OAuth2 client credentials endpoint",
        "tags": ["protected"],
        "security": [
          {
            "oauth2ClientCreds": ["admin"]
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created"
          }
        }
      }
    },
    "/openid-protected": {
      "get": {
        "operationId": "getOpenIdProtected",
        "summary": "OpenID Connect protected endpoint",
        "tags": ["protected"],
        "security": [
          {
            "openIdConnect": ["openid", "profile"]
          }
        ],
        "responses": {
          "200": {
            "description": "OpenID protected data"
          }
        }
      }
    },
    "/multi-auth": {
      "get": {
        "operationId": "getMultiAuth",
        "summary": "Multiple auth options (OR logic)",
        "tags": ["protected"],
        "security": [
          {
            "apiKeyHeader": []
          },
          {
            "bearerAuth": []
          },
          {
            "basicAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Data accessible via any auth method"
          }
        }
      }
    },
    "/combined-auth": {
      "get": {
        "operationId": "getCombinedAuth",
        "summary": "Combined auth requirements (AND logic)",
        "tags": ["protected"],
        "security": [
          {
            "apiKeyHeader": [],
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Data requiring both auth methods"
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "apiKeyHeader": {
        "type": "apiKey",
        "name": "X-API-Key",
        "in": "header",
        "description": "API key in header"
      },
      "apiKeyQuery": {
        "type": "apiKey",
        "name": "api_key",
        "in": "query",
        "description": "API key in query parameter"
      },
      "basicAuth": {
        "type": "http",
        "scheme": "basic",
        "description": "HTTP Basic authentication"
      },
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "JWT Bearer token authentication"
      },
      "oauth2ClientCreds": {
        "type": "oauth2",
        "description": "OAuth2 client credentials flow",
        "flows": {
          "clientCredentials": {
            "tokenUrl": "https://auth.example.com/token",
            "scopes": {
              "admin": "Full admin access",
              "read": "Read-only access"
            }
          }
        }
      },
      "oauth2AuthCode": {
        "type": "oauth2",
        "description": "OAuth2 authorization code flow",
        "flows": {
          "authorizationCode": {
            "authorizationUrl": "https://auth.example.com/authorize",
            "tokenUrl": "https://auth.example.com/token",
            "scopes": {
              "read": "Read access",
              "write": "Write access",
              "admin": "Admin access"
            }
          }
        }
      },
      "openIdConnect": {
        "type": "openIdConnect",
        "description": "OpenID Connect authentication",
        "openIdConnectUrl": "https://auth.example.com/.well-known/openid-configuration"
      }
    }
  }
}
